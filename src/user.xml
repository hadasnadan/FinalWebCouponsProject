<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="il.ac.shenkar.finalproject.model.User" table="users">
		<id name="id" column="id">
		</id>
		<property name="name">
			<column name="name" />
		</property>
		
		<property name="pass">
			<column name="pass" />
		</property>
	</class>
</hibernate-mapping>